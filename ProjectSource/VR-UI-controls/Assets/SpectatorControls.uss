.grid-container {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: flex-start;
    align-items: flex-start;
    vertical-scrolling: hidden;
}


.button {
    width: 100px; /* Set the desired width */
    height: 100px; /* Set the desired height */
    padding: 0;
    margin: 0;
    border: none;
}

.button img {
    width: 100%;
    height: 100%;
    object-fit: cover;
}